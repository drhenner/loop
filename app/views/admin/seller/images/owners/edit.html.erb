<h1> Edit Owner </h1>

<%= form_for(@owner, :url => admin_seller_images_owner_path(@owner),
                        :html => {:class => 'prepend-4', :multipart => true}) do |f| %>
  <%= render :partial => 'form', :locals => {:f => f} %>
  <div class="button generic_submit_button">
    <%= submit_tag "Create",
                    :class  => "button",
                    :id     => "create_owner_button" %>
  </div>
<% end %>

  <%= link_to 'Show', admin_seller_images_owner_path( @owner ), :class => 'button' %> |
